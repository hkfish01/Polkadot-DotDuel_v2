# Contracts Environment Variables
# Copy this file to .env and fill in the values

# Deployer private key (without 0x prefix)
DEPLOYER_PRIVATE_KEY=

# Platform wallet address (receives fees if any)
PLATFORM_WALLET=

# Oracle address (can declare match results)
ORACLE_ADDRESS=

# Mantle Sepolia Testnet (default for development)
MANTLE_SEPOLIA_RPC_URL=https://rpc.sepolia.mantle.xyz

# Mantle Mainnet (for production)
MANTLE_RPC_URL=https://rpc.mantle.xyz

# Mantle Explorer API Key (for contract verification)
MANTLE_EXPLORER_API_KEY=xyz
